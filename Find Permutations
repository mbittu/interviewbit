vector<int> Solution::findPerm(const string A, int B) 
{
    int m=A.size();
    int l=1;
    int r=B;
    int i=0;
    vector<int>ans;
    while(i<m)
    {
        if(A[i]=='I')
        {
            ans.push_back(l);
            l++;
        }
        else
        {
            ans.push_back(r);
            r--;
        }
        i++;
    }
    ans.push_back(l);
    return ans;
}
