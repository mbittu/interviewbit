int Solution::coverPoints(vector<int> &A, vector<int> &B)
{
    int step=0;
    if(A.size()<2 || B.size()<2)
        return 0;
    for(int i=0;i<A.size()-1;i++)
    {
        int a=A[i];
        int b=A[i+1];
        int c=B[i];
        int d=B[i+1];
        int x=abs(a-b);
        int y=abs(c-d);
        step+=max(x,y);
    }
    return step;
}
