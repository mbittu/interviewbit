void Solution::nextPermutation(vector<int> &A) 
{
    int n=A.size();
    int i;
    for(i=n-1;i>0;i--)
    {
        if(A[i]>A[i-1])
            break;
    }
    if(i==0)
    {
        sort(A.begin(),A.end());
        return;
    }
    int x=A[i-1];
    int k=i;
    for(int j=i+1;j<n;j++)
    {
        if(A[j]>x && A[j]<A[k])
            k=j;
    }
    int temp=A[i-1];
    A[i-1]=A[k];
    A[k]=temp;
    sort(A.begin()+i,A.end());
}
